[ignore]
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/example/.*
<PROJECT_ROOT>/local/.*

[include]
<PROJECT_ROOT>/src/*
<PROJECT_ROOT>/test/*
<PROJECT_ROOT>/tool/*

[libs]
tool/flow-typed
tool/flow-def

[lints]

[options]
include_warnings=true
module.name_mapper='^#src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^#test\/\(.*\)$' -> '<PROJECT_ROOT>/test/\1'
module.name_mapper='^#tool\/\(.*\)$' -> '<PROJECT_ROOT>/tool/\1'

[strict]

[version]
0.128.0

