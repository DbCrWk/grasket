{
    "plugins": [
        [
            "babel-plugin-module-resolver",
            {
                "alias": {
                    "#src": "./src",
                    "#test": "./test",
                    "#tool": "./tool"
                }
            }
        ]
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": true
                }
            }
        ],
        "@babel/preset-flow"
    ],
    "sourceMaps": "inline",
    "retainLines": true
}
